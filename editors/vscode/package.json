{
  "name": "barq-coder-vscode",
  "displayName": "BarqCoder",
  "description": "BarqCoder VSCode Extension - The Lightning Fast Agentic AI Coder",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning"
  ],
  "activationEvents": [
    "onLanguage:rust",
    "onLanguage:python",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "barqcoder.ask",
        "title": "Ask BarqCoder"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
